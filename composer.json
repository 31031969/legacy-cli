{
    "name": "platformsh/cli",
    "description": "Platform.sh CLI",
    "license": "MIT",
    "require": {
        "doctrine/cache": "~1.5",
        "guzzlehttp/ringphp": "^1.1",
        "platformsh/console-form": "0.0.7",
        "platformsh/client": "0.4.4",
        "symfony/console": "~2.5 >=2.5.2",
        "symfony/yaml": "~2.5",
        "symfony/finder": "~2.5",
        "symfony/filesystem": "~2.5",
        "symfony/process": "~2.5",
        "stecman/symfony-console-completion": "~0.6",
        "symfony/event-dispatcher": "^2.7",
        "padraic/phar-updater": "^1.0"
    },
    "suggest": {
        "drush/drush": "For Drupal projects"
    },
    "autoload": {
        "files": ["constants.php"],
        "psr-4": {
            "Platformsh\\Cli\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Platformsh\\Cli\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.3 <4.8",
        "mikey179/vfsStream": "~1.4",
        "drush/drush": "~7.0.0"
    },
    "authors": [
        {
            "name": "Bojan Zivanovic"
        },
        {
            "name": "Patrick Dawkins"
        }
    ],
    "bin": [
        "bin/platform"
    ]
}
