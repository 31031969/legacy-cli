{
    "name": "platformsh/cli",
    "description": "Platform.sh CLI",
    "license": "MIT",
    "require": {
        "php": ">=7.2.5",
        "doctrine/cache": "^1.7",
        "platformsh/console-form": ">=0.0.29 <2.0",
        "platformsh/client": "^2.6@beta",
        "symfony/console": "^5.4",
        "symfony/yaml": "^5.4",
        "symfony/finder": "^5.4",
        "symfony/filesystem": "^5.4",
        "symfony/process": "^5.4",
        "stecman/symfony-console-completion": "~0.10",
        "symfony/event-dispatcher": "^5.4",
        "padraic/phar-updater": "^1.0",
        "symfony/dependency-injection": "^5.4",
        "symfony/config": "^5.4",
        "ext-json": "*",
        "composer/ca-bundle": "^1.3",
        "psr/log": "^1.1"
    },
    "suggest": {
        "drush/drush": "For Drupal projects"
    },
    "autoload": {
        "files": ["constants.php"],
        "psr-4": {
            "Platformsh\\Cli\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Platformsh\\Cli\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "bamarni/composer-bin-plugin": "^1.2"
    },
    "authors": [
        {
            "name": "Platform.sh"
        },
        {
            "name": "Patrick Dawkins"
        },
        {
            "name": "Bojan Zivanovic"
        }
    ],
    "bin": [
        "bin/platform"
    ],
    "config": {
        "platform": {
            "php": "7.2.5"
        },
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "scripts": {
        "update-countries": "php scripts/update-countries.php"
    }
}
